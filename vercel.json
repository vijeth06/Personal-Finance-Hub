{
  "version": 2,
  "builds": [
    {
      "src": "Personal Finance Hub/server.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Personal Finance Hub/server.js"
    },
    {
      "src": "/(.*\\.(js|css|html|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/Personal Finance Hub/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Personal Finance Hub/server.js"
    }
  ],
  "functions": {
    "Personal Finance Hub/server.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}